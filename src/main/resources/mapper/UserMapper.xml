<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lk11.dao.UserDAO">

	<select id="selectList" resultType="com.lk11.vo.UserVO">
		SELECT * FROM USER
	</select>
	
	<select id="selectOne" parameterType="java.lang.String" resultType="com.lk11.vo.UserVO">
		SELECT * FROM USER WHERE id = #{id}
	</select>

</mapper>